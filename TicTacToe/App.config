<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="TicTacToeServerURI" value="tcp://DEVMAC:8080/TicTacToeEndpoint"/>
  </appSettings>
  <system.runtime.remoting>
    <application>
      <channels>
        <channel ref="tcp"  port="0" >
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>
